#include <iostream>
#include <string>

using namespace std;

string elements(char input)
{
	string stuff[6] = { "He","Lr","W","O","Rg","Gd" };
	
	
	if (input == ' ')
		return " ";
	else
	{
		for (int i = 0; i < 6; i++)
		{
			for (int j = 0; j < 2; j++)
			{
				if (input == stuff[i][j])
				{
					return stuff[i];
				}
			}
		}
	}
}

int main()
{
	string helloWorld = "Hello World";

	for (int i = 0; i < helloWorld.size(); i++)
		cout<<elements(helloWorld[i]);
	

	cout << endl;
	cout << helloWorld;

	cin.get();
	cin.get();
	return 0;
}
